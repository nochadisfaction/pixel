{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "pixelated"
    },
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "eastus"
    },
    "appServicePlanSku": {
      "value": "B1"
    },
    "staticWebAppSku": {
      "value": "Free"
    },
    "enableAzureOpenAI": {
      "value": true
    },
    "enableStorage": {
      "value": true
    },
    "enableMonitoring": {
      "value": true
    },
    "customDomain": {
      "value": "https://pixelatedempathy.com"
    },
    "githubRepoUrl": {
      "value": "https://github.com/nochadisfaction/pixel"
    },
    "githubBranch": {
      "value": "master"
    }
  }
